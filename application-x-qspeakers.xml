<?xml version="1.0" encoding="UTF-8"?>
<mime-info xmlns="http://www.freedesktop.org/standards/shared-mime-info">
   <mime-type type="application/x-qspeakers">
     <sub-class-of type="application/xml"/>
     <icon name="qspeakers"/>
     <comment>Loudspeaker project file</comment>
     <comment xml:lang="fr">Projet d'enceinte acoustique</comment>
     <magic priority="60">
       <match value="QSpeakersProject" type="string" offset="10"/>
     </magic>
     <glob pattern="*.qsp"/>
   </mime-type>
</mime-info>
